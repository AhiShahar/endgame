{
  "name": "chess",
  "version": "0.1.0",
  "description": "practive in React DnD",
  "scripts": {
    "build": "webpack --config ./webpack.prod.config.js ",
    "dev": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "test": "jest --coverage --testMatch '**/*-spec.js'",
    "test-watch": "jest --watch --testMatch '**/tests/*-spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/HIVERY/space_race.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "collectCoverageFrom": [
        "src/**/*.js",
        "src/**/*.jsx"
      ]
    }
  },
  "author": "Ahi Shahar <ahi@hivery.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HIVERY/space_race/issues"
  },
  "homepage": "https://github.com/HIVERY/space_race#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.22.0",
    "babelify": "^7.3.0",
    "css-loader": "^0.28.11",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-fetch-mock": "^1.2.1",
    "jsdom": "^9.10.0",
    "jshint": "^2.9.4",
    "node-sass": "^4.5.3",
    "react-router-dom": "^4.1.1",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.5",
    "sinon": "^1.17.7",
    "style-loader": "^0.21.0",
    "watchify": "^3.8.0",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "npm": "^5.7.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dnd-touch-backend": "^0.3.12",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1"
  }
}
